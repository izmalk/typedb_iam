match $p1 isa person, has full-name $pn1; $p2 isa person, has full-name $pn2; $ac isa action; $f isa file, has path "README.md"; {$ac has action-name "modify_file";} or {$ac has action-name "view_file";}; not {$p1 has full-name "Masako Holley";}; not {$p2 has full-name "Masako Holley";}; $a($f, $ac) isa access; $pe1($p1, $a) isa permission; $pe2($p2, $a) isa permission; get $f;
